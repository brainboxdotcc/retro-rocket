GLOBAL LIB$ = "/programs/libraries" ' Default library path

PRINT "Mounting ";
COLOR 14
PRINT "filesystems";
COLOR 7
PRINT "..."

MOUNT "/devices", "", "devfs" ' Mount filesystems
MOUNT "/boot", "hd0", "fat32"

KEYMAP "qemu"

REPEAT
	PRINT "Launching ";
	COLOR 14
	PRINT "shell";
	COLOR 7
	PRINT "..."
	CHAIN "/programs/rocketsh"
	PRINT "Shell process ended."
UNTIL FALSE
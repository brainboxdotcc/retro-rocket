REM Socket test
HOST$ = "neuron.brainbox.cc"
PRINT "Connecting to "; HOST$; " ";
IP$ = DNS$(HOST$)
PRINT " (IP: "; IP$; ")... ";

CONNECT FD, IP$, 80
PRINT "Connected! Socket descriptor="; FD

SOCKWRITE FD, "GET /test.txt HTTP/1.1"; CHR$(13); CHR$(10); "Host: "; IP$; CHR$(13); CHR$(10); CHR$(13); CHR$(10);

SOCKREAD FD, TEST$
PRINT TEST$

PRINT "CLOSING"

SOCKCLOSE FD

PRINT "CLOSED"
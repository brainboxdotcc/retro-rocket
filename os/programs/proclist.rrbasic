COLOR 15
PRINT "PID", "PPID", "CPUID", "PROCESS NAME"
COLOR 7
PROCline()
FOR Y = 0 TO GETPROCCOUNT - 1
	PRINT GETPROCID(Y), GETPROCPARENT(Y), GETPROCCPUID(Y);
	COLOR 15
	PRINT "", LEFT$(GETPROCNAME$(Y), 60)
	COLOR 7
NEXT
PROCline()
COLOR 15
PRINT GETPROCCOUNT; " Total Processes"
COLOR 7
END


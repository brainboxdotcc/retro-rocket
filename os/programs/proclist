REM Process List
REM ------------

COLOR 15
PRINT "PID", "PPID", "CPUID", "PROCESS NAME"
COLOR 7
PROCline()
FOR Y = 0 TO GETPROCCOUNT
	PRINT GETPROCID(Y), GETPROCPARENT(Y), GETPROCCPUID(Y);
	COLOR 15
	PRINT "", LEFT$(GETPROCNAME$(Y), 60)
	COLOR 7
NEXT
PROCline()
COLOR 15
PRINT GETPROCCOUNT; " Total Processes"
COLOR 7
END

DEF PROCline()
	FOR X = 0 TO TERMWIDTH
		PRINT "â”€";
	NEXT
	PRINT ""
PROCEND